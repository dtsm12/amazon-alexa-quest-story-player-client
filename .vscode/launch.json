{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Launch",
         "type": "node",
         "request": "launch",
         "program": "${workspaceRoot}\\node_modules\\mocha\\bin\\_mocha",
         "stopOnEntry": false,
         "args": [
            "${workspaceRoot}\\test\\**\\*.js"            
         ],
         "cwd": "${workspaceRoot}",
         "runtimeExecutable": null,
         "runtimeArgs": [
            "--nolazy"
         ],
         "env": {
            "NODE_ENV": "development"
         },
         "externalConsole": false,
         "sourceMaps": false,
         "outDir": null
      }
   ]
}